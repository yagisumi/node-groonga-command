    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-terser": "^5.3.0",
    "rollup-plugin-typescript2": "^0.27.0",
    "ts-jest": "^25.4.0",
    "typedoc": "^0.17.6",
    "typescript": "^3.8.3"
  },
  "dependencies": {
    "deep-equal": "^2.0.3"
  }
}
