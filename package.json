    "@types/deep-equal": "^1.0.1",
    "@types/jest": "^25.2.1",
    "@types/node": "^13.13.4",
    "@typescript-eslint/eslint-plugin": "^2.30.0",
    "@typescript-eslint/parser": "^2.30.0",
    "coveralls": "^3.1.0",
    "eslint": "^6.8.0",
    "jest": "^25.5.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.0.5",
    "rimraf": "^3.0.2",
