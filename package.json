{
  "name": "@yagisumi/groonga-command",
  "version": "0.1.0",
  "description": "",
  "keywords": [],
  "engines": {
    "node": ">= 6.0.0"
  },
  "private": true,
  "main": "lib/groonga-command",
  "module": "lib/groonga-command.mjs",
  "types": "lib/groonga-command.d.ts",
  "unpkg": "umd/groonga-command.js",
  "scripts": {
    "test": "run-s prettier:fix eslint jest",
    "jest": "jest",
    "doc": "typedoc --json ./docs/typedoc.json --tsconfig ./tsconfig.json ./src",
    "build": "run-s clean build:*",
    "clean": "rimraf lib coverage umd *.tgz .rpt2_cache",
    "build:cjs": "rollup -c -f cjs -o ./lib/groonga-command.js",
    "build:mjs": "rollup -c -f es -o ./lib/groonga-command.mjs",
    "build:umd": "rollup -c rollup.config.umd.js",
    "build:d_ts": "tsc -p src -d --emitDeclarationOnly --outDir ./lib",
    "prettier:fix": "prettier --write **/*.{ts,js,json}",
    "eslint": "eslint **/*.{js,ts}",
    "prepack": "run-s build test"
  },
  "author": "yagisumi",
  "repository": "github:yagisumi/node-groonga-command",
  "homepage": "http://github.com/yagisumi/node-groonga-command#readme",
